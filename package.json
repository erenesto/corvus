{
  "name": "raven-css",
  "version": "1.0.0",
  "description": "A clean, grayish CSS library",
  "main": "dist/raven.css",
  "repository": "https://github.com/erenesto/raven-css.git",
  "author": {
    "name": "Eren Guldas",
    "email": "erenguldas@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "run-s clean && parcel examples/index.pug --open",
    "build": "run-s test clean parcel:build minify",
    "parcel:build": "parcel build -o dist/raven.css scss/raven.scss  --no-minify",
    "minify": "cleancss -o dist/raven.min.css dist/raven.css",
    "clean": "rimraf dist",
    "lint:scss": "stylelint -s --fix --formatter=verbose 'scss/**/*.scss'",
    "format:scss": "prettier-stylelint --write --q 'scss/**/*.scss'",
    "test": "run-s format:scss lint:scss"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "prettier": {},
  "keywords": [
    "clean design",
    "clean css",
    "css",
    "gray",
    "grayish",
    "raven",
    "frontend",
    "ui"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "pug": "^2.0.4",
    "rimraf": "^3.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-declaration-strict-value": "^1.1.8",
    "stylelint-order": "^4.0.0"
  },
  "dependencies": {}
}
